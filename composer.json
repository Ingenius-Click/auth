{
    "name": "ingenius/auth",
    "description": "Authentication package for Ingenius",
    "type": "library",
    "license": "proprietary",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Ingenius Team",
            "email": "info@ingenius.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ingenius/core": "*",
        "laravel/framework": "^12.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ingenius\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ingenius\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ingenius\\Auth\\Providers\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
